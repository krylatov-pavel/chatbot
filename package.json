{
  "name": "chatbot-server",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && npm run start",
    "server": "json-server --watch ./server/db.json --port 4000",
    "dev": "concurrently --kill-others \"npm run client\" \"npm run server\""
  },
  "dependencies": {
    "json-server": "^0.14.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  }
}
